func fib(var x)
{
    if (x == 0)
    {
        return 0$
    }
    else if (x == 1)
    {
        return 1$
    }
    else
    {
        return call fib(x - 1) + call fib(x - 2)$
    }
}

main
{
    call print(call fib(6))$
    call printNewLine()$
}
